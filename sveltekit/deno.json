{
  "version": "0.1.0",
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "imports": {
    "@sveltejs/adapter-auto": "npm:@sveltejs/adapter-auto@^4.0.0",
    "@sveltejs/kit": "npm:@sveltejs/kit@^2.16.0",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^5.0.0",
    "@tailwindcss/forms": "npm:@tailwindcss/forms@^0.5.9",
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.15",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.0",
    "svelte": "npm:svelte@^5.0.0",
    "svelte-check": "npm:svelte-check@^4.0.0",
    "tailwindcss": "npm:tailwindcss@^4.0.0",
    "vite": "npm:vite@^6.0.0"
  }
}
